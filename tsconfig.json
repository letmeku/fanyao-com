{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types"],
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "react-nexlif": ["src"],
      "react-nexlif/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*", "typings.d.ts"]
}
